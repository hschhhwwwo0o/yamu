{
  "name": "yamu",
  "version": "5.0.0",
  "description": "The application is designed for creating mock-ups. Create product mock-ups with the online mock-up generator. Simply select a mock-up, upload your design and download a watermark-free image.",
  "scripts": {
    "dev": "cd ./app/yamu-react-app && npm run dev",
    "build": "bash ./scripts/build_app.bash",
    "start": "cd ./app/yamu-react-app && npm run start",
    "build-modules": "bash ./scripts/build_modules.bash",
    "lint": "bash ./scripts/lint.bash",
    "prepare": "husky install"
  },
  "type": "module",
  "keywords": [
    "web application",
    "mock-up create"
  ],
  "author": "@hschhhwwwo0o",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.3"
  },
  "dependencies": {
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.4"
  }
}
