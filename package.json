{
  "name": "yamu",
  "version": "2.0.0",
  "description": "The application is designed for creating mock-ups. Create product mock-ups with the online mock-up generator. Simply select a mock-up, upload your design and download a watermark-free image.",
  "scripts": {
    "pre-commit": "npm run _lint-modules",
    "dev": "cd ./app/yamu-react-app && npm run dev",
    "build": "cd ./app/yamu-react-app && npm run build",
    "start": "cd ./app/yamu-react-app && npm run start",
    "re-build-modules": "npm run _build-mock-up-generator-module && npm run _build-mock-up-html-renderer-module",
    "_lint-modules": "npm run _lint-mock-up-generator-module && npm run _lint-mock-up-html-renderer-module",
    "_build-mock-up-generator-module": "cd ./modules/mock-up-generator && npm run compile",
    "_lint-mock-up-generator-module": "cd ./modules/mock-up-generator && npm run lint",
    "_build-mock-up-html-renderer-module": "cd ./modules/mock-up-html-renderer && npm run compile",
    "_lint-mock-up-html-renderer-module": "cd ./modules/mock-up-html-renderer && npm run lint"
  },
  "type": "module",
  "keywords": [
    "web application",
    "mock-up create"
  ],
  "author": "@hschhhwwwo0o",
  "publisher": "@hschhhwwwo0o",
  "homepage": "https://github.com/hschhhwwwo0o/yamu",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.3"
  }
}
