{
  "name": "yamu",
  "version": "1.0.0",
  "description": "Web application for mock-up creation",
  "scripts": {
    "pre-commit": "npm run lint-modules",
    "dev": "cd ./app/yamu-react-app && npm run dev",
    "build": "cd ./app/yamu-react-app && npm run build",
    "start": "cd ./app/yamu-react-app && npm run start",
    "re-build-modules": "npm run build-mock-up-generator-module && npm run build-mock-up-html-renderer-module",
    "lint-modules": "npm run lint-mock-up-generator-module && npm run lint-mock-up-html-renderer-module",
    "build-mock-up-generator-module": "cd ./modules/mock-up-generator && npm run compile",
    "lint-mock-up-generator-module": "cd ./modules/mock-up-generator && npm run lint",
    "build-mock-up-html-renderer-module": "cd ./modules/mock-up-html-renderer && npm run compile",
    "lint-mock-up-html-renderer-module": "cd ./modules/mock-up-html-renderer && npm run lint"
  },
  "type": "module",
  "keywords": [
    "web application",
    "mock-up create"
  ],
  "author": "@hschhhwwwo0o",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.3"
  }
}
